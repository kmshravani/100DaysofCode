height = float(input("enter your height in m: "))
weight = float(input("enter your weight in kg: "))
# ğŸš¨ Don't change the code above ğŸ‘†

#Write your code below this line ğŸ‘‡
h = float(height)**2 
total = float(weight)/float(h)
t = round(total,2)
if(t < 18.5):
  print(f"Your BMI is {int(t)}, you are underweight.")
elif(18.5 < t < 25):
  print(f"Your BMI is {int(t)}, you have normal weight.")
elif(25 < t < 30):
  print(f"Your BMI is {int(t)}, you are slightly overweight.")
elif(30 < t < 35):
  print(f"Your BMI is {int(t)}, you are obese.")
else:
  print(f"Your BMI is {int(t)}, you are clinically obese.")
