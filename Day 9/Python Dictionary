travel_log = [
{
  "country": "France",
  "visits": 12,
  "cities": ["Paris", "Lille", "Dijon"]
},
{
  "country": "Germany",
  "visits": 5,
  "cities": ["Berlin", "Hamburg", "Stuttgart"]
},
]
#ðŸš¨ Do NOT change the code above

#TODO: Write the function that will allow new countries
#to be added to the travel_log. ðŸ‘‡
def add_new_country(user_country,user_visits,user_cities):
  new_dictionary = {}
  new_dictionary["country"] = user_country
  new_dictionary["visits"] = user_visits
  new_dictionary["cities"] = user_cities
  travel_log.append(new_dictionary)
  
#ðŸš¨ Do not change the code below ,"visits"=user_visits,"cities"=user_cities ["country"]["country"]["visits"]["cities"]
add_new_country("Russia", 2, ["Moscow", "Saint Petersburg"])
print(travel_log)



